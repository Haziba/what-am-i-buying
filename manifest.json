{
  "manifest_version": 2,

  "name": "What am I buying?",
  "description": "This extension tells you what's what about your Tesco shop",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
    "js": ["jquery-3.2.1.min.js"]
  }]

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://code.jquery.com/",
  ]
}
